
@article{jayender_modeling_2008,
	title = {Modeling and {Control} of {Shape} {Memory} {Alloy} {Actuators}},
	volume = {16},
	issn = {1063-6536},
	doi = {10.1109/TCST.2007.903391},
	abstract = {This brief describes a new model for shape memory alloy (SMA) actuators based on the physics of the process and develops control strategies using the model. The model consists of three equations - the temperature dynamics described by Joules heating-convectional cooling, the mole fraction distribution with temperature given by statistics to describe a two state system, and a constitutive equation relating the changes in temperature and mole fraction to the stress and strain induced in the SMA. This model is used to develop two control schemes for controlling the strain in an SMA actuator. The first control scheme describes a gain-scheduled proportional-integral (PI) controller, the gains of which are obtained by means of linear quadratic regulator (LQR) optimization. The second control scheme is an Hinfin loop-shaping controller using normalized coprime stabilization which ensures robust stability by minimizing the effect of unmodeled dynamics at high frequencies. Simulation and experimental results show fast and accurate control of the strain in the SMA actuator for both control schemes.},
	number = {2},
	journal = {IEEE Transactions on Control Systems Technology},
	author = {Jayender, J. and Patel, R. V. and Nikumb, S. and Ostojic, M.},
	month = mar,
	year = {2008},
	keywords = {constitutive equation, control system synthesis, Fermi-Dirac principle, Fermi–Dirac principle, gain scheduling, gain-scheduled proportional-integral controller, Hinfin control, Hinfin loop-shaping controller, intelligent actuators, Joules heating-convectional cooling, linear quadratic control, linear quadratic regulator (LQR) optimization, linear quadratic regulator optimization, mole fraction distribution, normalized coprime stabilization, optimisation, PI control, PI controller, robust control, robust loop-shaping H-infinity controller, robust stability, shape memory alloy (SMA), shape memory alloy actuators, shape memory effects, statistics, temperature dynamics, two state system},
	pages = {279--287},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\sethomas\\Zotero\\storage\\KNG6BA34\\4425377.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\sethomas\\Zotero\\storage\\VK9LBCGX\\Jayender et al. - 2008 - Modeling and Control of Shape Memory Alloy Actuato.pdf:application/pdf}
}

@article{majima_modeling_2001,
	title = {Modeling of shape memory alloy actuator and tracking control system with the model},
	volume = {9},
	issn = {1063-6536},
	doi = {10.1109/87.896745},
	abstract = {There has been great demand for shape memory alloy (SMA) actuators within the field of micro-machines. However, the thermomechanical behavior of SMA actuators is too complicated to accurately predict their response. Moreover the tracking control accuracy of SMA actuators is limited due to the inherent hysteresis nonlinearity. The paper presents a model of an SMA actuator for use in mechanical design and a control system for compensating the hysteresis. First, the model based on constitutive laws developed by Brinson et al. (1996) and our proposed hysteresis model is demonstrated. Next, we propose a control system composed of a proportional integral derivative (PID) feedback loop and a feedforward loop. In the feedforward loop, the desired control input corresponding to the desired displacement is obtained from a static SMA model derived by modifying the classic Preisach hysteresis model. We also propose a method for identifying parameters of the hysteresis model. This methods demands only measurement of the major loop. Application of the control system to our developed SMA actuator shows that the tracking control performance is improved},
	number = {1},
	journal = {IEEE Transactions on Control Systems Technology},
	author = {Majima, S. and Kodama, K. and Hasegawa, T.},
	month = jan,
	year = {2001},
	keywords = {Actuators, control nonlinearities, Control system synthesis, Control systems, feedback, feedforward, feedforward loop, hysteresis, Hysteresis, hysteresis nonlinearity, intelligent actuators, mechanical design, microactuators, PD control, Pi control, Preisach hysteresis model, Proportional control, proportional integral derivative feedback loop, Shape control, shape memory alloy actuator, Shape memory alloys, shape memory effects, thermomechanical behavior, Thermomechanical processes, three-term control, tracking, tracking control system},
	pages = {54--59},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\sethomas\\Zotero\\storage\\R3AUBNXY\\896745.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\sethomas\\Zotero\\storage\\YZDS7GWL\\Majima et al. - 2001 - Modeling of shape memory alloy actuator and tracki.pdf:application/pdf}
}

@article{qidwai_numerical_2000,
	title = {Numerical implementation of a shape memory alloy thermomechanical constitutive model using return mapping algorithms},
	volume = {47},
	url = {http://citeseerx.ist.psu.edu/viewdoc/download?doi=10.1.1.585.9848&rep=rep1&type=pdf},
	abstract = {Previous studies by the authors and their co-workers show that the structure of equations representing shape
Memory Alloy (SMA) constitutive behaviour can be very similar to those of rate-independent plasticity
models. For example, the Boyd–Lagoudas polynomial hardening model has a stress-elastic strain constitutive
relation that includes the transformation strain as an internal state variable, a transformation function
determining the onset of phase transformation, and an evolution equation for the transformation strain. Such
a structure allows techniques used in rate-independent elastoplastic behaviour to be directly applicable to
SMAs. In this paper, a comprehensive study on the numerical implementation of SMA thermomechanical
constitutive response using return mapping (elastic predictor-transformation corrector) algorithms is presented.
The closest point projection return mapping algorithm which is an implicit scheme is given special attention
together with the convex cutting plane return mapping algorithm, an explicit scheme already presented in an
earlier work. The closest point algorithm involves relatively large number of tensorial operations than the
cutting plane algorithm besides the evaluation of the gradient of the transformation tensor in the
ow rule and
the inversion of the algorithmic tangent tensor. A uni ed thermomechanical constitutive model, which does
not take into account reorientation of martensitic variants but uni es several of the existing SMA constitutive
models, is used for implementation. Remarks on numerical accuracy of both algorithms are given, and it is
concluded that both algorithms are applicable for this class of SMA constitutive models and preference can
only be given based on the computational cost. Copyright ? 2000 John Wiley \& Sons, Ltd.},
	journal = {INTERNATIONAL JOURNAL FOR NUMERICAL METHODS IN ENGINEERING},
	author = {Qidwai, M. A. and Lagoudas, D. C.},
	year = {2000},
	pages = {1123--1168},
	file = {Qidwai and Lagoudas - 2000 - Numerical implementation of a shape memory alloy t.pdf:C\:\\Users\\sethomas\\Zotero\\storage\\CQ9SDYIX\\Qidwai and Lagoudas - 2000 - Numerical implementation of a shape memory alloy t.pdf:application/pdf}
}

@article{yuan_review_2017,
