	pages = {021002--021002--11},
	file = {Full Text PDF:C\:\\Users\\sethomas\\Zotero\\storage\\575QXMZS\\Konh et al. - 2015 - Feasibility of Shape Memory Alloy Wire Actuation f.pdf:application/pdf}
