	keywords = {A. Phase transformation, A. Reorientation, B. Shape memory alloys, C. Constitutive modeling, C. Fischer–Burmeister function},
	pages = {30--54},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\sethomas\\Zotero\\storage\\Z9YQBU7A\\Auricchio et al. - 2014 - Theoretical and numerical modeling of shape memory.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\sethomas\\Zotero\\storage\\NUWRBPCE\\S0749641914000515.html:text/html}
}

@article{junker_variational_2015,
