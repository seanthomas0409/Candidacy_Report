	author = {Zhakypov, Z. and Paik, J.},
	year = {2018},
	keywords = {Compliant mechanisms, Fabrication, Geometry, Kinematics, multimodal locomotion, Plastics, robot design methodology, robotic origami, Robots, Shape, shape memory alloy (SMA)},
	pages = {1--15},
